<UserControl x:Class="ScheduleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Scheduler"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Width="100" Loaded="UserControl_Loaded">
    <Border x:Name="baseborder" BorderBrush="#7FB4B4B4" BorderThickness="0,0,1,0">
    <Grid x:Name="BaseGrid" Background="White"/>
    </Border>
</UserControl>
